PROGS = modpost.real-$(TYPE)

CFLAGS += -I$(top_srcdir)/include

include ../Makefile.inc

modpost.real-$(TYPE): file2alias.real-$(TYPE).o modpost.real-$(TYPE).o sumversion.real-$(TYPE).o
